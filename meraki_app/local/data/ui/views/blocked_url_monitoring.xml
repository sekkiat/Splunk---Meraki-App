        <option name="charting.lineWidth">2</option>
        <option name="refresh.display">progressbar</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <drilldown>
          <link target="_blank">/app/meraki_app/blocked_url_audit_activities?form.sc=$click.value$&amp;form.timerange.earliest=$earliest$&amp;form.timerange.latest=$latest$&amp;form.ap=$ap$</link>
        </drilldown>
      </chart>
    </panel>
    <panel>
      <title>Blocked URL by Category</title>
